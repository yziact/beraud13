      <template id="report_invoice_document_inherit_sale_stock" inherit_id="report_invoice_document">
            <xpath expr="//td[@name='partner_ref_head']" position="after">
                <td t-if="o.incoterms_id">
                    <strong>Incoterms:</strong>
                </td>
            </xpath>
            <xpath expr="//td[@name='partner_ref_data']" position="after">
                <td  t-if="o.incoterms_id">
                    <p t-field="o.incoterms_id.code"/>
                </td>
            </xpath>
        </template>
    </data>
</openerp>
